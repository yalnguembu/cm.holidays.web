    "@vuelidate/core": "^2.0.2",
    "@vuelidate/validators": "^2.0.2",
    "pinia": "^2.1.4",
    "typescript": "^5.2.2",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/test-utils": "^2.2.7",
    "@vue/tsconfig": "~0.4.0",
    "autoprefixer": "^10.4.13",
    "axios": "~1.5.1",
    "form-data": "~4.0.0",
    "happy-dom": "^8.1.4",
    "openapi-typescript-codegen": "^0.24.0",
    "postcss": "^8.4.20",
    "tailwindcss": "^3.2.4",
    "vite": "^4.0.0",
    "vitest": "^0.27.1",
    "vue-tsc": "~1.8.19"
